{
    "name": "svr/core",
    "description": "SVR CORE - A simple core package for SVR",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dedermus/svr_core",
    "authors": [
        {
            "name": "dedermus",
            "email": "dedermus@yandex.ru"
        }
    ],
    "autoload": {
        "classmap": [
            "src/",
            "src/Exceptions"
        ],
        "psr-4": {
            "Svr\\Core\\": "src/",
            "Svr\\Core\\Seeders\\": "database/seeders/",
            "Svr\\Core\\Exceptions\\": "src/Exceptions"
        },
        "files": ["src/bootstrap.php"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Svr\\Core\\CoreServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "dedermus/open-admin-core": "^v2.0",
        "stefangabos/zebra_image": "^2.8",
        "hisorange/browser-detect": "^5.0",
        "laravel/sanctum": "^4.0",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    }
}

